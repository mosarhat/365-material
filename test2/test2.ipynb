    "# binary search O(logn)\n",
    "\n",
    "def binary_search(arr, target):\n",
    "    n = len(arr)\n",
    "    if n <= 4:\n",
    "        for elem in arr:\n",
    "            if elem == target:\n",
    "                return True\n",
    "        return False\n",
    "    else:\n",
    "        low = 0\n",
    "        high = len(arr)\n",
    "        mid = (low + high) // 2\n",
    "        left = arr[:mid]\n",
    "        right = arr[mid:]\n",
    "        if target <= arr[mid]:\n",
    "            return binary_search(left, target)\n",
    "        else:\n",
    "            return binary_search(right, target)"
