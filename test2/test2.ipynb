    "## quicksort \n",
    "\n",
    "def quicksort(a):\n",
    "    if len(a) <= 1:\n",
    "        return a\n",
    "    else:\n",
    "        pivot = a[(len(a) - 1) // 2]\n",
    "        less, equal, greater = [], [], []\n",
    "        for i in a:\n",
    "            if i < pivot:\n",
    "                less.append(i)\n",
    "            elif i > pivot:\n",
    "                greater.append(i)\n",
    "            else:\n",
    "                equal.append(i)\n",
    "        return quicksort(less) + equal + quicksort(greater)\n",
    "\n",
    "quicksort([3,2,1]) "
